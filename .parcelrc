  "resolvers": [
    "@parcel/resolver-glob",
    "..."
  ],
  "reporters": [
    "...",
    "parcel-reporter-static-files-copy"
  ],
  "transformers": {
    "*.geojson": [
      "parcel-transformer-geojson"
    ],
    "*.md": [
      "parcel-transformer-markdown-front-matter"
    ],
  }
}